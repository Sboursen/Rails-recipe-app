<div class="flex flex-col mt-6 gap-12 w-[50rem] mx-auto">
    <h1>Shopping List</h1>
    <div>
    <p></p>
    <p></p>
    </div>
  <table class="table-auto">
    <thead>
      <tr class="bg-gray-500 text-white">
        <th>Food</th>
        <th>Quantity</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <% 6.times do |food| %>
        <tr class="odd:bg-gray-200">
          <td>Rice</td>
          <td>500 grams</td>
          <td>$750</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
